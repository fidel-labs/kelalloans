          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".contacts-box .contacts-text:not(.last-child) when (@showContacts)": {
              "padding-bottom": "5px"
            },
            ".img-box when (@showLogo), (@showBrand)": {
              "align-items": "center"
            },
            ".img-wrap when (@showLogo) and (@showBrand) and (@showLogoSocial)": {
              "margin-right": "15px"
            },
            "& when (@showForm)": {
              "input, textarea": {
                "color": "@labelsColor",
                "background": "contrast(@labelsColor, darken(@labelsColor, 50%), #ffffff, 60%) !important",
                "border": "1px solid lighten(@labelsColor, 88%)",
                "&:focus": {
                  "border-color": "@secondaryColor"
                }
              },
              "input::placeholder": {
                "color": "lighten(@labelsColor, 46%)"
              }
            },
            ".social .iconfont-wrapper when (@showSocial) and (@showLogoSocial)": {
              "width": "~\"@{iconsSize}rem\"",
              "height": "~\"@{iconsSize}rem\"",
              "border-color": "@iconsColor !important",
              ".amp-iconfont": {
                "color": "contrast(@iconsColor, #222222, #ffffff, 15%)",
                "width": "(@iconsSize * 0.42rem)",
                "font-size": "(@iconsSize * 0.42rem)",
                "line-height": "(@iconsSize - 0.25rem)"
              },
              "background": "@iconsColor !important",
              "margin-bottom": "10px",
              "& when ((@iconsSize * 16) > 30)": {
                "@media (max-width: 767px)": {
                  "width": "30px",
                  "line-height": "26px"
                }
              },
              "&:hover": {
                "color": "@primaryColor !important"
              }
            },
            ".form-block when (@transparentForm) and (@showForm)": {
              "padding": "0"
            },
            ".form-wrap .mbr-overlay when not (@transparentForm) and (@showForm)": {
              "background-color": "@formBgColor",
              "opacity": "@formBgOpacity"
            },
            ".img-wrap when (@showLogo) and (@showLogoSocial)": {
              "width": "~\"@{widthLogo}%\"",
              "min-width": "60px"
            },
            "@media (max-width: 991px)": {
              ".mbr-row > .md-pb": {
                "padding-bottom": "30px"
              }
            },
            "@media (max-width: 767px)": {
              ".mbr-row > :not(.last-child)": {
                "padding-bottom": "30px"
              }
            },
            ".mbr-section-title:after when (@showLineTitle) and (@showTitle)": {
              "content": "\"\"",
              "display": "block",
              "width": "40px",
              "height": "1px",
              "margin-top": "10px",
              "background-color": "@primaryColor",
              "& when (@align = '2')": {
                "margin-left": "auto",
                "margin-right": "auto"
              },
              "& when (@align = '3')": {
                "margin-left": "auto",
                "@media (max-width: 767px)": {
                  "margin-right": "auto"
                }
              }
            },
            ".mbr-list .mbr-text when (@showCategories)": {
              "line-height": "1",
              "&:before when (@showMarkers)": {
                "content": "''",
                "display": "inline-block",
                "vertical-align": "middle",
                "width": "~\"@{markersSize}rem\"",
                "height": "~\"@{markersSize}rem\"",
                "border-radius": "50%",
                "margin-right": "15px",
                "background-color": "@markersColor"
              },
              "& when (@showEffect)": {
                "transition": "all 0.3s",
                "&:hover": {
                  "padding-left": "20px",
                  "color": "@primaryColor"
                }
              }
            }
          },
          "_name": "footer1",
          "_sourceTheme": "innovationamp",
          "_customHTML": "<section class=\"footer1\" group=\"Footers\" plugins=\"amp-main, amp-form, amp-mustache\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" condition=\"showTitle || (showContacts && (showAddress || showPhone || showEmail)) || showCategories || (showForm && (showTitle || showFormText)) || (showLogoSocial && (showLogo || showSocial || showLogoText))\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showTitle || (showContacts && (showAddress || showPhone || showEmail)) || showCategories || (showForm && (showTitle || showFormText)) || (showLogoSocial && (showLogo || showSocial || showLogoText))\">\n    <header condition=\"showForm || showContacts || showCategories\">Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked condition=\"showForm || showContacts || showCategories\">\n        <input type=\"checkbox\" title=\"Line Title\" name=\"showLineTitle\" condition=\" showTitle && (showForm || showContacts || showCategories)\" checked>\n        <input type=\"checkbox\" title=\"Logo and Social\" name=\"showLogoSocial\" checked>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" checked>\n        <input type=\"checkbox\" title=\"Categories\" name=\"showCategories\">\n        <input type=\"checkbox\" title=\"Form\" name=\"showForm\">\n        <select title=\"Align\" name=\"align\" condition=\"showTitle || (showContacts && (showAddress || showPhone || showEmail)) || showCategories || (showForm && (showTitle || showFormText)) || (showLogoSocial && (showLogo || showSocial || showLogoText))\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header condition=\"showLogoSocial\">Logo and Social</header>\n        <input type=\"checkbox\" name=\"showLogo\" title=\"Logo\" condition=\"showLogoSocial\">\n            <input type=\"range\" inline title=\"Width\" name=\"widthLogo\" min=\"10\" max=\"100\" value=\"40\" step=\"1\" condition=\"showLogoSocial && showLogo\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" condition=\"showLogoSocial\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showLogoText\" condition=\"showLogoSocial\" checked> \n        <input type=\"checkbox\" name=\"showSocial\" title=\"Social\" condition=\"showLogoSocial\"> \n            <input type=\"range\" inline title=\"Count\" name=\"socialCount\" min=\"1\" max=\"10\" value=\"4\" step=\"1\" condition=\"showSocial && showLogoSocial\">\n            <input type=\"range\" inline title=\"Size\" name=\"iconsSize\" min=\"1.5\" max=\"5\" step=\"0.1\" value=\"2.7\" condition=\"showSocial && showLogoSocial\">\n            <input type=\"color\" title=\"Color\" name=\"iconsColor\" value=\"#05325c\" condition=\"showSocial && showLogoSocial\">\n    <header condition=\"showContacts\">Contacts</header>\n        <input type=\"checkbox\" name=\"showAddress\" title=\"Address\" condition=\"showContacts\" checked>\n        <input type=\"checkbox\" name=\"showPhone\" title=\"Phone\" checked condition=\"showContacts\">\n        <input type=\"checkbox\" name=\"showEmail\" title=\"E-mail\" checked condition=\"showContacts\">\n    <header condition=\"showCategories\">Categories</header>\n        <input type=\"range\" title=\"Count\" name=\"categoriesCount\" min=\"1\" max=\"10\" step=\"1\" value=\"3\" condition=\"showCategories\">\n        <input type=\"checkbox\" title=\"Markers\" name=\"showMarkers\" checked>\n        <input type=\"range\" inline title=\"Size\" name=\"markersSize\" min=\"0.1\" max=\"1.5\" step=\"0.1\" value=\"0.4\" condition=\"showCategories && showMarkers\">\n        <input type=\"color\" title=\"Color\" name=\"markersColor\" value=\"#ffffff\" condition=\"showMarkers && showCategories\">\n        <input type=\"checkbox\" title=\"Show Effect Categorie\" name=\"showEffect\" checked>\n    <header condition=\"showForm\">Form</header>\n        <input type=\"checkbox\" title=\"Text\" name=\"showFormText\" condition=\"showForm\" checked>  \n        <input type=\"color\" title=\"Form Text Color\" name=\"labelsColor\" value=\"#000000\" condition=\"showForm\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparentForm\" condition=\"showForm\" checked>    \n            <input type=\"color\" title=\"Form Bg Color\" name=\"formBgColor\" value=\"#ffffff\" condition=\"!transparentForm && showForm\">\n            <input type=\"range\" title=\"Form Bg Opacity\" name=\"formBgOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"1\" condition=\"!transparentForm && showForm\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\"> \n        <input type=\"range\" inline title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"twoBgColor && bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value>\n            <input type=\"color\" title=\"Background Color\" value=\"#194f82\" selected>\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#f4f5f7\" condition=\"twoBgColor && bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type !== 'color') || (twoBgColor && bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 && twoBgColor && bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 && twoBgColor && bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid': fullWidth}\">\n        <div class=\"mbr-row mbr-jc-c\" mbr-class=\"{'mbr-px-2': fullWidth, 'mbr-row-reverse':reverseContent && (showTitle || (showContacts && (showAddress || showPhone || showEmail)) || showCategories || (showForm && (showTitle || showFormText)) || (showLogoSocial && (showLogo || showSocial || showLogoText))), 'align-center': align == 2, 'align-right': align == 3}\">\n            <div class=\"logoSocial-box mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12 last-child': !showContacts && !showCategories && !showForm, 'mbr-col-lg-3 mbr-col-md-6 md-pb': showContacts && showCategories &&  showForm, 'mbr-col-md-6': (showContacts && !showCategories && !showForm) || (!showContacts && !showCategories && showForm) || (!showContacts && showCategories && !showForm), 'mbr-col-md-4': (showContacts && showCategories && !showForm) || (showContacts && !showCategories && showForm) || (!showContacts && showCategories && showForm)}\" mbr-if=\"showLogoSocial\">\n                <div class=\"img-box mbr-flex\" mbr-if=\"showLogo || showBrand\" mbr-class=\"{'mbr-m-auto': align == 2 && showBrand && showLogo, 'mbr-ml-auto': align == 3 && showBrand && showLogo}\">\n                    <div class=\"img-wrap\" mbr-if=\"showLogo\" mbr-class=\"{'mbr-m-auto': align == 2 && !showBrand, 'mbr-ml-auto': align == 3 && !showBrand}\">\n                        <amp-img src=\"@PROJECT_PATH@/assets/images/logo3.png\" layout=\"responsive\" width=\"16\" height=\"9\" alt=\"Mobirise\" class=\"mobirise-loader\">\n                            <div placeholder class=\"placeholder\"></div>\n                            <img class=\"mbr-temp\" src=\"@PROJECT_PATH@/assets/images/logo3.png\" alt=\"Mobirise\" title>\n                        </amp-img>\n                    </div>\n                    <span mbr-if=\"showBrand\" mbr-buttons mbr-theme-style=\"display-5\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\"><a class=\"navbar-caption mbr-bold text-primary\" data-app-selector=\".navbar-caption\" href=\"https://mobiri.se\" data-app-placeholder=\"Type Text\" mbr-link-type=\"name\">Bank</a></span>\n                </div>\n                <p class=\"mbr-text mbr-white\" mbr-theme-style=\"display-7\" data-app-selector=\".logoSocial-box .mbr-text\" mbr-if=\"showLogoText\" mbr-class=\"{'mbr-pt-2': showLogo || showBrand}\"><br>Kelal Loans is not lender and only charges fees for its service of facilitating the loan eligibility assessment.&nbsp;<br><br>Visit our Privacy Policy page to see how we handle your data. Basically we will not use or sell your data to any other party.<br><br>Kelal Loans is brought to you by <a href=\"https://www.fidel-labs.co\" class=\"text-primary\">Fidel Labs</a></p>\n                <div class=\"icons-list social\" mbr-if=\"showSocial\" mbr-list mbr-list-grow=\"socialCount\" mbr-link-type=\"socials\" mbr-class=\"{'mbr-pt-2': showLogoText || showLogo || showBrand}\">\n                    <a href=\"http://www.youtube.com/channel/UCt_tncVAetpK5JeM8L-8jyw\" target=\"_blank\" class=\"iconfont-wrapper\">\n                        <span mbr-icon class=\"amp-iconfont fa-youtube fa\"></span>\n                    </a>\n                    <a href=\"https://www.facebook.com/pages/Mobirise/1616226671953247\" target=\"_blank\" class=\"iconfont-wrapper\">\n                        <span mbr-icon class=\"amp-iconfont fa-facebook-square fa\"></span>\n                    </a>\n                    <a href=\"https://plus.google.com/u/0/+Mobirise/posts\" target=\"_blank\" class=\"iconfont-wrapper\">\n                        <span mbr-icon class=\"amp-iconfont fa-google-plus-circle fa\"></span>\n                    </a>\n                    <a href=\"https://www.linkedin.com/in/mobirise\" target=\"_blank\" class=\"iconfont-wrapper\">\n                        <span mbr-icon class=\"amp-iconfont fa-linkedin-square fa\"></span>\n                    </a>\n                </div>\n            </div>\n            <div class=\"contacts-box mbr-col-sm-12 mbr-white\" mbr-class=\"{'mbr-col-md-12': !showLogoSocial && !showCategories && !showForm, 'mbr-col-lg-3 mbr-col-md-6 md-pb': showLogoSocial && showCategories && showForm, 'mbr-col-md-6': (showLogoSocial && !showCategories && !showForm) || (!showLogoSocial && !showCategories && showForm) || (!showLogoSocial && showCategories && !showForm), 'mbr-col-md-4': (showLogoSocial && showCategories && !showForm) || (showLogoSocial && !showCategories && showForm) || (!showLogoSocial && showCategories && showForm), 'last-child': !showCategories && !showForm}\" mbr-if=\"showContacts\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\" mbr-class=\"{'mbr-pb-4': showAddress || showPhone || showEmail}\">CONTACTS</h3>\n                <p class=\"mbr-text contacts-text mbr-fonts-style\" mbr-if=\"showAddress\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-class=\"{'last-child': !showPhone && !showEmail}\"><span mbr-link-type=\"address\">Sintayehu Belay Building - 1st Floor</span></p>\n                <p class=\"mbr-text contacts-text mbr-fonts-style\" mbr-if=\"showPhone\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\" mbr-class=\"{'last-child': !showPhone}\"><span mbr-link-type=\"phone\">+251 993 506 048</span></p>\n                <p class=\"mbr-text contacts-text mbr-fonts-style last-child\" mbr-if=\"showEmail\" mbr-theme-style=\"display-7\" data-app-selector=\".contacts-text\"><span mbr-link-type=\"email\">hello@kelal-loans.com</span></p>  \n            </div>\n            <div class=\"categories-box mbr-col-sm-12 mbr-white\" mbr-class=\"{'mbr-col-md-12': !showLogoSocial && !showContacts && !showForm, 'mbr-col-lg-3 mbr-col-md-6': showLogoSocial && showContacts && showForm, 'mbr-col-md-6': (showLogoSocial && !showContacts && !showForm) || (!showLogoSocial && !showContacts && showForm) || (!showLogoSocial && showContacts && !showForm), 'mbr-col-md-4': (showLogoSocial && showContacts && !showForm) || (showLogoSocial && !showContacts && showForm) || (!showLogoSocial && showContacts && showForm), 'last-child': !showForm}\" mbr-if=\"showCategories\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\" mbr-class=\"{'mbr-pb-4': showAddress || showPhone || showEmail}\">CATEGORIES</h3>\n                <div class=\"mbr-list\" mbr-list mbr-list-grow=\"categoriesCount\">\n                    <p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\">\n                        Lorem ipsum</p>\n                    <p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\">\n                        Lorem ipsum</p>\n                    <p class=\"mbr-text mbr-fonts-style mbr-py-1\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-list .mbr-text\">\n                        Lorem ipsum</p>\n                </div>\n            </div>\n            <div mbr-form class=\"form-box mbr-col-sm-12 mbr-white last-child\" mbr-class=\"{'mbr-col-md-12': !showLogoSocial && !showContacts && !showCategories, 'mbr-col-lg-3 mbr-col-md-6': showLogoSocial && showContacts && showCategories, 'mbr-col-md-6': (showLogoSocial && !showContacts && !showCategories) || (!showLogoSocial && !showContacts && showCategories) || (!showLogoSocial && showContacts && !showCategories), 'mbr-col-md-4': (showLogoSocial && showContacts && !showCategories) || (showLogoSocial && !showContacts && showCategories) || (!showLogoSocial && showContacts && showCategories)}\" mbr-if=\"showForm\">\n                <h3 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-section-title\" mbr-if=\"showTitle\" mbr-class=\"{'mbr-pb-4': showAddress || showPhone || showEmail}\">SUBSCRIBE</h3>\n                <p class=\"mbr-text mbr-pb-3 mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showFormText\" data-app-selector=\".form-box .mbr-text\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>    \n                <div class=\"form-block\">\n                    <form class=\"mbr-form\" action method=\"post\" name=\"Mobirise Form\" target=\"_top\" novalidate>\n                        <div class=\"mbr-overlay\" mbr-if=\"!transparentForm\"></div>\n                        <div submit-success>\n                            <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n                        </div>\n                        <div submit-error>\n                            <template data-form-alert type=\"amp-mustache\">Subscription successful!</template>\n                        </div>\n                        <div class=\"fieldset\">\n                            <input type=\"email\" name=\"form[data][0][1]\" data-form-field=\"E-mail\" placeholder=\"E-mail Address\" required>\n                        </div>\n                        <div class=\"mbr-section-btn\" data-toolbar=\"-mbrBtnMove, -mbrBtnAdd, -mbrLink\" mbr-buttons mbr-theme-style=\"display-7\">\n                            <a type=\"submit\" class=\"btn btn-sm btn-primary btn-form\">Send</a>\n                        </div>\n                    </form>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
