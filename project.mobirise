          "_name": "tab2",
          "_customHTML": "<section class=\"tab2\" group=\"Tabs & Toggles\" plugins=\"amp-main, amp-selector\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected>Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Text</header>\n        <input type=\"range\" inline title=\"Width Image\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons\">\n        <input type=\"range\" inline title=\"Height Image\" name=\"heightImg\" min=\"5\" max=\"60\" step=\"1\" value=\"25\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked>\n        <input type=\"checkbox\" title=\"Buttons\" name=\"showCardButtons\" checked>\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\">\n            <option value=\"1\" selected>Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\" checked>\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg && (showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone)\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"!transparentBg && (showCardTitle || showCardSubtitle || showCardList1 || showCardList2 || showCardButtons || showCardText || showMilestone)\">\n    <header>List1</header>   \n        <input type=\"checkbox\" title=\"List1\" name=\"showCardList1\">\n        <input type=\"range\" inline title=\"Width Counters\" name=\"widthImg1Counters\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 1 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 1 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 1 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 1 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\" checked=\"checked\">\n        <input type=\"range\" inline title=\"Width Markers\" name=\"widthImg1Markers\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 2 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 2 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 2 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 2 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 2 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 2 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <input type=\"range\" inline title=\"Width Icons\" name=\"widthImg1Icons\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"2.5\" condition=\"showCardList1 &amp;&amp; ((stylizedCounters1Tab1 == 3 &amp;&amp; list1CountTab1>0) || (stylizedCounters1Tab2 == 3 &amp;&amp; list1CountTab2>0) || (stylizedCounters1Tab3 == 3 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2) || (stylizedCounters1Tab4 == 3 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3) || (stylizedCounters1Tab5 == 3 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4) || (stylizedCounters1Tab6 == 3 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5))\">\n        <!--Tab1-->\n            <input type=\"range\" inline title=\"Count Tab1\" name=\"list1CountTab1\" min=\"0\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList1\"> \n            <select title=\"Stylized Counters Tab1\" name=\"stylizedCounters1Tab1\" condition=\"showCardList1 &amp;&amp; list1CountTab1>0\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected>Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab1\" name=\"showCardList1TitleTab1\" condition=\"showCardList1 &amp;&amp; list1CountTab1>0\">\n            <input type=\"checkbox\" title=\"Text List Tab1\" name=\"showCardList1TextTab1\" checked condition=\"showCardList1 &amp;&amp; list1CountTab1>0\">\n        <!--Tab2-->\n            <input type=\"range\" inline title=\"Count Tab2\" name=\"list1CountTab2\" min=\"0\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList1\">\n            <select title=\"Stylized Counters Tab2\" name=\"stylizedCounters1Tab2\" condition=\"showCardList1 &amp;&amp; list1CountTab2>0\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected>Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab2\" name=\"showCardList1TitleTab2\" condition=\"showCardList1 &amp;&amp; list1CountTab2>0\">\n            <input type=\"checkbox\" title=\"Text List Tab2\" name=\"showCardList1TextTab2\" checked condition=\"showCardList1 &amp;&amp; list1CountTab2>0\">\n        <!--Tab3-->\n            <input type=\"range\" inline title=\"Count Tab3\" name=\"list1CountTab3\" min=\"0\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList1 &amp;&amp; tabCount > 2\">\n            <select title=\"Stylized Counters Tab3\" name=\"stylizedCounters1Tab3\" condition=\"showCardList1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected>Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab3\" name=\"showCardList1TitleTab3\" condition=\"showCardList1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2\">\n            <input type=\"checkbox\" title=\"Text List Tab3\" name=\"showCardList1TextTab3\" condition=\"showCardList1 &amp;&amp; list1CountTab3>0 &amp;&amp; tabCount > 2\" checked=\"checked\">\n        <!--Tab4-->\n            <input type=\"range\" inline title=\"Count Tab4\" name=\"list1CountTab4\" min=\"0\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList1 &amp;&amp; tabCount > 3\">\n            <select title=\"Stylized Counters Tab4\" name=\"stylizedCounters1Tab4\" condition=\"showCardList1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected>Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab4\" name=\"showCardList1TitleTab4\" condition=\"showCardList1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3\">\n            <input type=\"checkbox\" title=\"Text List Tab4\" name=\"showCardList1TextTab4\" checked condition=\"showCardList1 &amp;&amp; list1CountTab4>0 &amp;&amp; tabCount > 3\">\n        <!--Tab5-->\n            <input type=\"range\" inline title=\"Count Tab5\" name=\"list1CountTab5\" min=\"0\" max=\"9\" step=\"1\" value=\"2\" condition=\"showCardList1 &amp;&amp; tabCount > 4\">\n            <select title=\"Stylized Counters Tab5\" name=\"stylizedCounters1Tab5\" condition=\"showCardList1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4\">\n                <option value=\"1\" selected>Counters</option>\n                <option value=\"2\">Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab5\" name=\"showCardList1TitleTab5\" condition=\"showCardList1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4\">\n            <input type=\"checkbox\" title=\"Text List Tab5\" name=\"showCardList1TextTab5\" checked condition=\"showCardList1 &amp;&amp; list1CountTab5>0 &amp;&amp; tabCount > 4\">\n        <!--Tab6-->\n            <input type=\"range\" inline title=\"Count Tab6\" name=\"list1CountTab6\" min=\"0\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList1 &amp;&amp; tabCount > 5\">\n            <select title=\"Stylized Counters Tab6\" name=\"stylizedCounters1Tab6\" condition=\"showCardList1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\">Markers</option>\n                <option value=\"3\" selected>Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Title List Tab6\" name=\"showCardList1TitleTab6\" condition=\"showCardList1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5\">\n            <input type=\"checkbox\" title=\"Text List Tab6\" name=\"showCardList1TextTab6\" checked condition=\"showCardList1 &amp;&amp; list1CountTab6>0 &amp;&amp; tabCount > 5\">\n    <header>List2</header>\n        <input type=\"checkbox\" title=\"List2\" name=\"showCardList2\">\n        <input type=\"range\" inline title=\"Width Marker\" name=\"widthImg2\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"1\" condition=\"showCardList2\">\n        <!--Tab1-->\n            <input type=\"range\" inline title=\"Count Tab1\" name=\"list2CountTab1\" min=\"0\" max=\"9\" step=\"1\" value=\"3\" condition=\"showCardList2\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab1\" condition=\"showCardList2 &amp;&amp; list2CountTab1 > 0\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select>\n        <!--Tab2-->\n            <input type=\"range\" inline title=\"Count Tab2\" name=\"list2CountTab2\" min=\"0\" max=\"9\" step=\"1\" value=\"3\" condition=\"showCardList2\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab2\" condition=\"showCardList2 &amp;&amp; list2CountTab2 > 0\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select>\n        <!--Tab3-->\n            <input type=\"range\" inline title=\"Count Tab3\" name=\"list2CountTab3\" min=\"0\" max=\"9\" step=\"1\" value=\"3\" condition=\"showCardList2 &amp;&amp; tabCount > 2\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab3\" condition=\"showCardList2 &amp;&amp; list2CountTab3 > 0 &amp;&amp; tabCount > 2\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select> \n        <!--Tab4-->\n            <input type=\"range\" inline title=\"Count Tab4\" name=\"list2CountTab4\" min=\"0\" max=\"9\" step=\"1\" value=\"2\" condition=\"showCardList2 &amp;&amp; tabCount > 3\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab4\" condition=\"showCardList2 &amp;&amp; list2CountTab4 > 0 &amp;&amp; tabCount > 3\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select>\n        <!--Tab5-->\n            <input type=\"range\" inline title=\"Count Tab5\" name=\"list2CountTab5\" min=\"0\" max=\"9\" step=\"1\" value=\"0\" condition=\"showCardList2 &amp;&amp; tabCount > 4\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab5\" condition=\"showCardList2 &amp;&amp; list2CountTab5 > 0 &amp;&amp; tabCount > 4\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select>\n        <!--Tab6-->\n            <input type=\"range\" inline title=\"Count Tab6\" name=\"list2CountTab6\" min=\"0\" max=\"9\" step=\"1\" value=\"0\" condition=\"showCardList2 &amp;&amp; tabCount > 5\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters2Tab6\" condition=\"showCardList2 &amp;&amp; list2CountTab6 > 0 &amp;&amp; tabCount > 5\">\n                <option value=\"1\">Line</option>\n                <option value=\"2\" selected>Marker</option>\n            </select>\n     <header>Milestone</header>   \n         <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\">\n            <input type=\"checkbox\" title=\"Border Top\" name=\"showBorderTop\" condition=\"showMilestone\">\n            <input type=\"checkbox\" title=\"Border Right\" name=\"showBorderRight\" condition=\"showMilestone &amp;&amp; columnWidth != 5 &amp;&amp; (milCountTab1 > 1 || milCountTab2 > 1 || milCountTab3 > 1 || milCountTab4 > 1 || milCountTab5 > 1 || milCountTab6 > 1)\" checked=\"\">\n            <input type=\"color\" title=\"Color Border\" name=\"borderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; (showBorderTop || (showBorderRight &amp;&amp; columnWidth != 5 &amp;&amp; (milCountTab1 > 1 || milCountTab2 > 1 || milCountTab3 > 1 || milCountTab4 > 1 || milCountTab5 > 1 || milCountTab6 > 1)))\">\n            <input type=\"range\" inline title=\"Count Milestone Tab1\" name=\"milCountTab1\" min=\"0\" max=\"4\" step=\"1\" value=\"0\" condition=\"showMilestone\">\n            <input type=\"range\" inline title=\"Count Milestone Tab2\" name=\"milCountTab2\" min=\"0\" max=\"4\" step=\"1\" value=\"0\" condition=\"showMilestone\">\n            <input type=\"range\" inline title=\"Count Milestone Tab3\" name=\"milCountTab3\" min=\"0\" max=\"4\" step=\"1\" value=\"0\" condition=\"showMilestone &amp;&amp; tabCount > 2\">\n            <input type=\"range\" inline title=\"Count Milestone Tab4\" name=\"milCountTab4\" min=\"0\" max=\"4\" step=\"1\" value=\"0\" condition=\"showMilestone &amp;&amp; tabCount > 3\">\n            <input type=\"range\" inline title=\"Count Milesto