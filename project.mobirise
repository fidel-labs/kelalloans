      "components": [
        {
          "_styles": {
            "background-color": "@menuBgColor",
            "overflow": "visible",
            ".navbar": {
              "background": "@menuBgColor"
            },
            "& when (@showLogo)": {
              ".navbar-brand amp-img": {
                "height": "~\"@{logoHeight}px\"",
                "width": "~\"@{logoWidth}px\""
              }
            },
            "& when (@showBrand)": {
              ".navbar-caption": {
                "line-height": "inherit !important"
              }
            },
            ".navbar .navbar-collapse": {
              "@media (max-width: 991px)": {
                "background": "@menuBgColor",
                "& when (@sidebarSide = 'left')": {
                  "right": "auto",
                  "left": "0"
                }
              }
            },
            "& when (@showItems)": {
              ".nav-link": {
                "margin": ".667em 1em !important",
                "padding": "0 !important"
              },
              ".dropdown-menu": {
                "background": "@menuBgColor"
              },
              ".dropdown-item": {
                "position": "relative",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "left": "-15px",
                  "top": "50%",
                  "transform": "translateY(-50%) rotate(45deg)",
                  "opacity": "0",
                  "margin-right": "15px",
                  "width": "5px",
                  "height": "5px",
                  "border-top": "1px solid @primaryColor",
                  "border-right": "1px solid @primaryColor"
                },
                "&,&:before": {
                  "transition": "all .3s"
                }
              },
              ".nav-item:hover": {
                ".nav-link, .dropdown-item:hover": {
                  "color": "@primaryColor !important"
                },
                ".nav-link:after": {
                  "border-color": "@primaryColor !important"
                }
              },
              ".dropdown-item, .nav-item:not(:last-child) .nav-link": {
                "border-bottom": "1px solid #bec1c7"
              },
              "@media (min-width: 992px)": {
                ".navbar-nav .nav-item .dropdown-item:hover": {
                  "background-color": "@secondaryColor !important",
                  "color": "contrast(@secondaryColor, darken(@secondaryColor, 50%), #ffffff, 60%) !important",
                  "padding": "0 30px 0 50px",
                  "&:before": {
                    "left": "30px",
                    "opacity": "1"
                  }
                },
                ".navbar-nav .nav-item .nav-link, .dropdown-menu .dropdown-item:last-child": {
                  "border": "none!important"
                },
                ".dropdown-menu .dropdown-item": {
                  "border-color": "#e4e5ee"
                }
              },
              "@media (max-width: 991px)": {
                ".dropdown:hover .nav-link": {
                  "background-color": "@secondaryColor !important",
                  "color": "contrast(@secondaryColor, darken(@secondaryColor, 50%), #ffffff, 60%) !important",
                  "&:after": {
                    "border-color": "contrast(@secondaryColor, darken(@secondaryColor, 50%), #ffffff, 60%) !important"
                  }
                },
                ".nav-link": {
                  "margin": "0!important",
                  "padding": "0 15px!important",
                  "width": "100%",
                  "line-height": "50px"
                }
              }
            },
            ".hamburger span": {
              "background-color": "@hamburgerColor"
            },
            ".builder-sidebar": {
              "background-color": "@menuBgColor"
            },
            ".close-sidebar": {
              "&:focus": {
                "outline": "2px auto @primaryColor"
              },
              "span": {
                "background-color": "@hamburgerColor"
              }
            },
            ".menu-social-list when (@showSocial)": {
              "& when (@showItems)": {
                "@media (min-width: 992px)": {
                  "padding-left": "15px"
                },
                "@media (max-width: 991px)": {
                  "padding-top": "10px"
                }
              },
              "& when (@showButtons)": {
                "@media (max-width: 991px)": {
                  "padding-bottom": "10px"
                }
              }
            }
          },
          "_name": "menu1",
          "_customHTML": "<section class=\"menu1 menu horizontal-menu\" group=\"Menu\" plugins=\"DropDown, amp-main, amp-sidebar\" always-top global once=\"menu\">\n\t<mbr-parameters>\n\t<!-- Block parameters controls (Blue \"Gear\" panel) -->\n\t<!--  -->\n\t<header>Size</header>\n\t<input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n\t<input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n\t<!--  -->\n\t<header>Show/Hide</header>\t\n\t<input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\" checked>\n\t<input type=\"range\" title=\"Logo Height\" inline name=\"logoHeight\" min=\"30\" max=\"60\" value=\"40\" step=\"1\" condition=\"showLogo\">\n\t<input type=\"range\" title=\"Logo Width\" inline name=\"logoWidth\" min=\"30\" max=\"60\" value=\"40\" step=\"1\" condition=\"showLogo\">\n\t<input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" checked>\n\t<input type=\"checkbox\" title=\"Show Menu Items\" name=\"showItems\" checked>\n\t<input type=\"checkbox\" title=\"Show Button(s)\" name=\"showButtons\" checked>\n\t<input type=\"checkbox\" title=\"Show Social\" name=\"showSocial\">\n\t<input type=\"range\" inline title=\"Icons Count\" name=\"socialCount\" min=\"1\" max=\"6\" value=\"3\" step=\"1\" condition=\"showSocial\">\n\t<header>Sidebar</header>\n\t<select title=\"Side\" name=\"sidebarSide\">\n\t\t<option value=\"left\">Left</option>\n\t\t<option value=\"right\" selected>Right</option>\n\t</select>\n\t<input type=\"color\" title=\"Hamburger Color\" name=\"hamburgerColor\" value=\"#194f82\">\n\t<header>Background</header>\n\t<input type=\"color\" title=\"Color\" name=\"menuBgColor\" value=\"#ffffff\">\n\t<!-- End block parameters -->\n\t</mbr-parameters>\n\t<!-- <div class=\"menu-wrapper\"> -->\n\t<nav class=\"navbar navbar-dropdown navbar-expand-lg\" mbr-class=\"{'navbar-fixed-top': sticky}\">\n\t\t<div class=\"menu-container\" mbr-class=\"{'container': !fullWidth,\n\t\t\t\t\t\t'container-fluid mbr-px-2': fullWidth,